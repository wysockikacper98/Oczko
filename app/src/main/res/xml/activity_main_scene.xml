<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="2000"
        motion:motionInterpolator="bounce"
        motion:autoTransition="animateToEnd">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="195dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="189dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/imageView"
            motion:layout_constraintBottom_toTopOf="parent" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/ekranStartowyTextView"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:layout_height="60dp"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView3"
            android:layout_width="50dp"
            android:id="@+id/imageView2"
            motion:pathMotionArc="startVertical"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toStartOf="@+id/imageView3"
            motion:layout_constraintHorizontal_chainStyle="packed" />
        <Constraint
            android:layout_height="60dp"
            android:layout_marginTop="24dp"
            android:layout_width="50dp"
            motion:layout_constraintTop_toBottomOf="@+id/ekranStartowyTextView"
            android:id="@+id/imageView3"
            motion:layout_editor_absoluteX="345dp"
            motion:layout_constraintStart_toEndOf="@+id/imageView2"
            motion:layout_constraintEnd_toEndOf="parent" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/ekranStartowyTextView"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/literki" >
        <Constraint
            android:layout_height="60dp"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView3"
            android:layout_width="50dp"
            android:id="@+id/imageView2"
            motion:layout_constraintEnd_toStartOf="parent" />
        <Constraint
            android:layout_height="60dp"
            android:layout_marginTop="24dp"
            android:layout_width="50dp"
            motion:layout_constraintTop_toBottomOf="@+id/ekranStartowyTextView"
            android:id="@+id/imageView3"
            motion:layout_constraintStart_toEndOf="parent" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/ekranStartowyTextView"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent" />
    </ConstraintSet>
</MotionScene>